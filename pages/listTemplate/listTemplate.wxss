
.tmpContainer {
  display: flex;
  flex-direction: column;
  border-bottom: 1rpx solid #eee;
}
.avatar_date {
  padding: 10rpx;
}
.avatar_date image {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.avatar_date text {
  vertical-align: middle;
}

.company {
  margin-left: 10rpx;
  font-size: 36rpx;
  font-weight: bold;

}

.tmpImg {
  width: 100%;
  height: 460rpx;
}

.content {
  font-size: 32rpx;
  text-indent: 2em;
}

.star_view_container image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.star_view_container text {
  font-size: 24rpx;
  vertical-align: middle;
  margin-right: 10rpx;

}